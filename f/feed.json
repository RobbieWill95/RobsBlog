---
layout: null
---
[
{% for post in site.posts %}
    {
      "title"    : "{{ post.title }}",
      "url"      : "https://weckman.me{{ post.url }}",
      "date"     : "{{ post.date | date: "%B %d, %Y" }}",
      "content"  : "{{ post.excerpt | strip_html| strip_newlines}}"
    } {% if forloop.last %}{% else %},{% endif %}
{% endfor %}
]
