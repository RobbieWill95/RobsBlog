---
layout: null
---
[
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title | escape }}",
      "lead"     : "{{ post.desc }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "https://www.weckman.me{{ post.url }}",
      "date"     : "{{ post.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
